
@{
    //ViewBag.AppTitle = null;
    Layout = null;
}

<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/bootstrap.min.css")"
          rel="stylesheet" />
    <style>
        body {
            padding-top: 60px;
        }
    </style>
</head>
<body>
    <nav class="navbar navbar-expand-sm bg-dark navbar-dark fixed-top">
        <a class="navbar-brand" href="#">@ViewBag.AppTitle</a>
        <ul class="navbar-nav">
            <li class="nav-item">
                <a class="nav-link" href="#">Link</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="#">Link</a>
            </li>
            <li class="nav-item">
                <!--id="menuPaises"-->
                @Html.ActionLink("Teste url", "Index", "TesteLayout", htmlAttributes: new { @class = "nav-link" })
            </li>
        </ul>
    </nav>
    
    <div class="container">
        @RenderBody()
    </div>
    <script src="@Url.Content("~/Scripts/jquery-3.6.0.min.js")">
    </script>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")">
    </script>
    <script src="@Url.Content("~/Scripts/modernizr-2.8.3.js")">
    </script>
    @RenderSection("script", required: false)
</body>
</html>


